<p id="notice"><%= notice %></p>

<h1>Listing Nutrients</h1>

<table>
  <thead>
    <tr>
      <th>Ref</th>
      <th>Name</th>
      <th>Group</th>
      <th>Unit</th>
      <th>Value</th>
      <th>Food</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nutrients.each do |nutrient| %>
      <tr>
        <td><%= nutrient.ref_id %></td>
        <td><%= nutrient.name %></td>
        <td><%= nutrient.group %></td>
        <td><%= nutrient.unit %></td>
        <td><%= nutrient.value %></td>
        <td><%= nutrient.food %></td>
        <td><%= link_to 'Show', nutrient %></td>
        <td><%= link_to 'Edit', edit_nutrient_path(nutrient) %></td>
        <td><%= link_to 'Destroy', nutrient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nutrient', new_nutrient_path %>
