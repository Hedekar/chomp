<h2>User List (<%= current_account.id %>)</h2>

<table border="1" style="width:100%">
  <tr>
    <th style="padding:10px">Account</th>
    <th style="padding:10px">Is Main</th>
    <th style="padding:10px">First Name</th>
    <th style="padding:10px">Last Name</th>
    <th style="padding:10px">Gender</th>
    <th style="padding:10px">Birth</th>
    <th style="padding:10px" colspan="3">Controls</th>
  </tr>
 
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td style="padding:10px"><%= user.account_id %></td>
        <td style="padding:10px"><%= user.is_main %></td>
        <td style="padding:10px"><%= user.first_name %></td>
        <td style="padding:10px"><%= user.last_name %></td>
        <td style="padding:10px"><%= user.gender %></td>
        <td style="padding:10px"><%= user.birth %></td>
        <td style="padding:10px"><%= link_to 'Show', user %></td>
        <td style="padding:10px"><%= link_to 'Edit', edit_user_path(user) %></td>
        <td style="padding:10px"><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>